{
  "expo": {
    "name": "movie-app",
    "slug": "movie-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "com.googleusercontent.apps.952889214145-2ll2tmakn8hl86bhpt8iha58n831ktd2",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.movieapp",
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification", "remote-notification"],
        "NSLocalNotificationUsageDescription": "This app will send you reminders for saved movies.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.movieapp"
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.952889214145-2ll2tmakn8hl86bhpt8iha58n831ktd2"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "webGoogleClientId": "952889214145-qgp6b9i0skrjcnl70l6o4v3isabflbv8.apps.googleusercontent.com",
      "iosGoogleClientId": "952889214145-2ll2tmakn8hl86bhpt8iha58n831ktd2.apps.googleusercontent.com",
      "router": {},
      "eas": {
        "projectId": "58bb9096-39d0-4142-a11f-7b621631df11"
      }
    }
  }
}
